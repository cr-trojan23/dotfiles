{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "https://vtop.vit.ac.in/vtop/*" ],
      "run_at": "document_start"
   } ],
   "description": "Easily see DA",
   "icons": {
      "48": "logo.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ExVTOP (Extended VTOP)",
   "permissions": [ "webRequest", "tabs", "https://vtop.vit.ac.in/vtop/*", "storage", "notifications", "identity", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.5.5"
}
