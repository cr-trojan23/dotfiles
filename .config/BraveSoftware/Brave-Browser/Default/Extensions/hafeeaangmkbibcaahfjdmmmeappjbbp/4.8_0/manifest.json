{
   "author": "Priyansh Jain",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "jquery.js", "jquery.BlockUI.js", "bitmaps.js", "coursepage.js", "internalmarkspage.js", "general.js", "captchaparser.js" ],
      "matches": [ "*://vtop.vit.ac.in/*" ]
   } ],
   "description": "Easier and user-friendly vtop",
   "key": "",
   "manifest_version": 2,
   "name": "Enhancer for VIT Vellore Academics",
   "page_action": {
      "default_popup": "popup.html"
   },
   "permissions": [ "webRequest", "downloads", "storage", "declarativeContent", "activeTab", "*://vtop.vit.ac.in/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.8",
   "web_accessible_resources": [ "styles/general/*.css", "styles/font/*.css", "styles/font/*.ttf" ]
}
